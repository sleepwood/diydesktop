{
  "source": "./src",
  "destination": "./esdoc",
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "undocumentIdentifier": {
        "enable": false
      },
      "includeSource": {
        "enable": false
      }
    }
  }, {
    "name": "esdoc-ecmascript-proposal-plugin",
    "option": {
      "all": true
    }
  }, {
    "name": "esdoc-inject-style-plugin",
    "option": {
      "styles": ["./docs/style.css"]
    }
  }]
}